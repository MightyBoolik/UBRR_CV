<script>
    $(document).ready(
            function() {
            /*    if(window.innerWidth < 480) {
                    onlineplusPopup.popup('hide');
                }*/
                var onlineplusPopup = $('#onlineplus-popup').popup({
                    closeelement: '.onlineplus-modal-close',
                    transition: 'all 0.3s'
                });
                
                if (!sessionStorage.getItem('rko_banner')) {
                    onlineplusPopup.popup('show');
                }
                
                var onlineplusPopupThankYou = $('#onlineplus-popup-a').popup({
                    closeelement: '.onlineplus-modal-close-a',
                    transition: 'all 0.3s'
                });

                // крестик event
                $('.onlineplus-modal-close').off("click").on('click', function () {
                    sessionStorage.setItem('rko_banner', '1');
                    uao.event('popup_close', {
                        'show': '262678688',
                        'popup_close': 1,
                        'utm_source': '',
                        'utm_medium': '1514',
                        'utm_campaing': 'close',
                        'element': 'x'
                    });
                });

                $('.button').off("click").on('click', function (event) {
                    sessionStorage.setItem('rko_banner', '1');
                  // event.preventDefault();
                   onlineplusPopup.popup('hide');
                  // onlineplusPopupThankYou.popup('show');
                    uao.event('send_claim_holding', {
                        'show':'262678688',
                        'utm_source':'',
                        'utm_medium':'1514',
                        'utm_campaing':'fix_call',
                        'element':'button_holding'
                    });
           /*         uao.event('click', {
                        'show': '262678688',
                        'utm_source': '',
                        'utm_medium': '1514',
                        'utm_campaing': 'click',
                        'element': 'more'
                    });*/
                });

                // shadow
                var name = '#onlineplus-popup';
                $(name + "_wrapper").off("click").on("click", function (event) {
                    var el = $(name);
                    if ($(el).data('popupoptions').blur && !$(event.target).closest(name).length && event.which !== 2 && $(event.target).is(':visible')) {
                        uao.event('popup_close', {
                            'show': '262678688',
                            'popup_close': 1,
                            'utm_source': '',
                            'utm_medium': '1514',
                            'utm_campaing': 'close',
                            'element': 'shadow'
                        });
                    }
                });
            });
